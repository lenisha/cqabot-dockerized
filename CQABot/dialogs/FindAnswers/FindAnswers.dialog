              "$kind": "Microsoft.EndDialog",
              "$designer": {
                "id": "qTQvem"
              }
            }
          ]
        },
        {
          "$kind": "Microsoft.RepeatDialog",
          "$designer": {
            "id": "5zUFbD"
          },
          "activityProcessed": true
        }
      ]
