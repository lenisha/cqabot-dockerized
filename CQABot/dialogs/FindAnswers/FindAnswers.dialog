          "body": {
            "question": "${user.question}",
            "top": 3,
            "confidenceScoreThreshold": 0.2
          },
          "headers": {
            "Ocp-Apim-Subscription-Key": "${settings.qna.endpointKey}"
          },
          "contentType": "application/json",
          "responseType": "json"
        },
        {
          "$kind": "Microsoft.IfCondition",
          "$designer": {
            "id": "OPOYdL"
          },
          "condition": "dialog.api_response.statusCode==200",
